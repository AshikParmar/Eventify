{
  "version": 2,
  "builds": [
    {
      "src": "api/cron-job.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/cron-job",
      "dest": "api/cron-job.js",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "dest": "index.js",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "PATCH",
        "OPTIONS"
      ],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "*",
        "Access-Control-Allow-Headers": "*"
      }
    }
  ],
  "crons": [
    {
      "path": "/api/cron-job",
      "schedule": "0 7 * * *"
    }
  ]
}
